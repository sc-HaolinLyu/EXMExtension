@{
    var ExmTools = ViewData["ExmTools"] as Dictionary<string, EXMExtension.Models.Tool>;
}
<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    @RenderSection("css", required: false)
</head>
<body>
<h1 class="head"> The Exm Tool Collection</h1>
<br/>
<ul class="nav nav-tabs">
    <li class="nav-item">
        <a class="nav-link active" aria-current="page" href="/ExmTool">home</a>
    </li>
    @if (ExmTools != null)
    {
        foreach (var key in ExmTools.Keys)
        {
            var tool = ExmTools[key];

            <li class="nav-item">
                <a class="nav-link" aria-current="page" href="/ExmTool/@tool.Action">@tool.Name</a>
            </li>
        }
    }

</ul>
    @RenderBody()
    @RenderSection("scripts", required: false)
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
<script>

    
</script>
</body>
</html>
